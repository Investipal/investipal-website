    },
    {
      "route": "/features/investment-policy-statements/",
      "redirect": "/features/investment-policy-statements",
      "statusCode": 301
    },
    {
      "route": "/features/automated-statement-scanner/",
      "redirect": "/features/automated-statement-scanner",
      "statusCode": 301
    },
    {
      "route": "/features/ai-driven-engagement/",
      "redirect": "/features/ai-driven-engagement",
      "statusCode": 301
    },
    {
      "route": "/features/asset-allocation/",
      "redirect": "/features/asset-allocation",
      "statusCode": 301
    },
    {
      "route": "/features/risk-management/",
      "redirect": "/features/risk-management",
      "statusCode": 301
    },
    {
      "route": "/integrations/precisefp/",
      "redirect": "/integrations/precisefp",
      "statusCode": 301
    },
    {
      "route": "/segments/financial-planners/",
