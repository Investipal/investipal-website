  "globalHeaders": {
    "x-content-type-options": "nosniff",
    "x-frame-options": "SAMEORIGIN",
    "referrer-policy": "strict-origin-when-cross-origin",
    "permissions-policy": "camera=(), microphone=(), geolocation=()"
  },
  "responseOverrides": {
    "404": {
      "redirect": "/404",
      "statusCode": 404
    }
  },
  "mimeTypes": {
    ".avif": "image/avif",
    ".webp": "image/webp",
    ".xml": "application/xml"
  }
}