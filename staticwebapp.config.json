    {
      "route": "/segments/wealth-managers/",
      "redirect": "/segments/wealth-managers",
      "statusCode": 301
    },
    {
      "route": "/risk-assessment/",
      "redirect": "/risk-assessment",
      "statusCode": 301
    },
    {
